#include "key/key.h"


//-------------------------------------------------------------------------------------------------------------------
// 函数简介       按键初始化
// 参数说明       void
// 返回参数       void
//-------------------------------------------------------------------------------------------------------------------
void key_init(void)
{
    //初始化引脚
    gpio_init();
    //外部引脚中断初始化
    exti_init();
}

